@model List<EMS.Models.SetUpDepartmentViewModel>
@{
    ViewData["Title"] = "Department Details";
    int count = 0;
}
<body class="bg-light">
    <div class="mt-5 card">
        <h1 class="bg-black text-light card-header py-4">Employee Data</h1>
        <table class="table card-body">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">SNo.</th>
                    <th scope="col">Department Name</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var data in Model)
                {

                    <tr>
                        <th scope="row">@(count = count + 1)</th>
                        <td>@data.DeptName</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>